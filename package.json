{
  "name": "tab-organizer",
  "displayName": "Tab Organizer",
  "version": "0.0.0",
  "description": "Chrome extension that sorts and organizes your browser tabs.",
  "type": "module",
  "keywords": [
    "chrome-extension",
    "tab-organizer",
    "tab-sort"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "format": "biome check --write .",
    "lint": "biome check .",
    "zip": "pnpm run build && node zip.js"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@crxjs/vite-plugin": "^2.3.0",
    "@types/chrome": "^0.1.36",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "gulp": "^5.0.1",
    "gulp-zip": "^6.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
